{
	"name": "emoji-code",
	"displayName": "Emoji Code",
	"description": "Snippets to insert escaped Emoji code into HTML, CSS, JavaScript/CoffeeScript/TypeScript/LiveScript, Python and Ruby",
	"version": "0.2.1",
	"publisher": "idleberg",
	"license": "MIT",
	"author": {
