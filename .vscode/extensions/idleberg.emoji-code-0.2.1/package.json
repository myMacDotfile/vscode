		"emoji",
		"smileys",
		"emoticons"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/idleberg/vscode-emoji-code.git"
	},
	"homepage": "https://github.com/idleberg/vscode-emoji-code",
	"bugs": {
		"url": "https://github.com/idleberg/vscode-emoji-code/issues"
	},
	"icon": "images/logo.png",
	"galleryBanner": {
		"color": "#f76964",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.0.0"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "css",
				"path": "./snippets/emoji-css.json"
			},
			{
